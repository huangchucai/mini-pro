<!--index.wxml-->
<view class="index-container">
    <view class="header">
        <v-publish-date index="{{classic.index}}"></v-publish-date>
        <v-like count="{{classic.fav_nums}}" isLike="{{classic.like_status}}" bind:like="onLike"></v-like>
    </view>
    <view class="content">
        <v-moive content="{{classic.content}}" backgroundImg="{{classic.image}}"></v-moive>
    </view>
</view>